{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "./www/js",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
  },
  "include": ["**/*.ts", "**/*.d.ts", "**/*.tsx"]
}